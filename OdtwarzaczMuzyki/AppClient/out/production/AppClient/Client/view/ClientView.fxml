<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane fx:id="rootPane2" maxHeight="533.0" maxWidth="600.0" minHeight="400.0" minWidth="440.0" prefHeight="533.0"
      prefWidth="440.0" style="-fx-background-color: #696969;" xmlns="http://javafx.com/javafx/8.0.172-ea"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="Client.view.ClientViewController">
    <children>
        <Button fx:id="connectButton" defaultButton="true" focusTraversable="false" layoutX="15.0" layoutY="133.0"
                mnemonicParsing="false" onMouseClicked="#connectButtonOnClicked" prefHeight="25.0" prefWidth="77.0"
                text="Connect"/>
        <Button fx:id="sendButton" defaultButton="true" focusTraversable="false" layoutX="349.0" layoutY="486.0"
                mnemonicParsing="false" onMouseClicked="#sendButtonOnClicked" prefHeight="25.0" prefWidth="69.0"
                text="Send"/>
        <TextArea fx:id="chatTextArea" editable="false" focusTraversable="false" layoutX="15.0" layoutY="178.0"
                  prefHeight="293.0" prefWidth="410.0"/>
        <TextField fx:id="messageTextField" layoutX="14.0" layoutY="486.0" prefHeight="25.0" prefWidth="324.0"
                   promptText="Message"/>
        <TextField fx:id="ipTextField" focusTraversable="false" layoutX="186.0" layoutY="133.0" prefHeight="25.0"
                   prefWidth="240.0" promptText="IP" text="localhost"/>
        <Button fx:id="disconnectButton" disable="true" defaultButton="true" focusTraversable="false" layoutX="99.0" layoutY="133.0"
                mnemonicParsing="false" onMouseClicked="#disconnectButtonOnClicked" prefHeight="25.0" prefWidth="77.0"
                text="Disconnect"/>
        <TextField alignment="CENTER" layoutX="10.0" layoutY="8.0" prefHeight="113.0" prefWidth="420.0"
                   promptText="KOMUNIKATOR MUZYCZNY" style="-fx-background-color: #696969;">
            <font>
                <Font name="Candara" size="30.0"/>
            </font>
        </TextField>
    </children>
</Pane>
